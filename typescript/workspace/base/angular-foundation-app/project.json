{
  "name": "workspace-base-angular-foundation-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "allors",
  "sourceRoot": "workspace/base/angular-foundation-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/workspace/base/angular-foundation-app",
        "index": "workspace/base/angular-foundation-app/src/index.html",
        "browser": "workspace/base/angular-foundation-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "workspace/base/angular-foundation-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "workspace/base/angular-foundation-app/src/favicon.ico",
          "workspace/base/angular-foundation-app/src/assets"
        ],
        "styles": ["workspace/base/angular-foundation-app/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "workspace-base-angular-foundation-app:build:production",
          "proxyConfig": "workspace/base/angular-foundation-app/proxy.conf.json"
        },
        "development": {
          "buildTarget": "workspace-base-angular-foundation-app:build:development",
          "proxyConfig": "workspace/base/angular-foundation-app/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "workspace-base-angular-foundation-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "workspace/base/angular-foundation-app/jest.config.ts"
      }
    }
  }
}
